  "nodemonConfig": {
    "execMap": {
      "js": "node --icu-data-dir=node_modules/full-icu"
    }
  },
  "scripts": {
    "build": "craco build",
    "audit": "yarn audit --json | node scripts/audit.js",
    "clean": "rm -rf build/*",
    "config": "node scripts/config.js",
    "config-check": "node scripts/config.js --check",
    "dev": "yarn run config-check && cross-env NODE_ENV=development REACT_APP_DEV_API_SERVER_PORT=3501 concurrently --kill-others \"yarn run dev-frontend\" \"yarn run dev-backend\"",
    "dev-frontend": "craco start",
    "dev-backend": "nodemon server/apiServer.js",
    "build": "react-app-rewired build",
    "eject": "craco eject"
    "format": "prettier --write '**/*.{js,css}'",
    "format-ci": "prettier --list-different '**/*.{js,css}'",
    "format-docs": "prettier --write '**/*.md'",
    "test": "craco test",
    "test-ci": "yarn run test-server --runInBand && react-app-rewired test --runInBand",
    "backend-start": "node backend.js",
    "start": "node server/apiServer.js",
    "dev-server": "cross-env NODE_ENV=development PORT=4000 REACT_APP_CANONICAL_ROOT_URL=http://localhost:4000 yarn run build && nodemon --watch server server/index.js",
    "test-server": "jest ./server/**/*.test.js",
    "heroku-postbuild": "yarn run build",
    "translate": "node scripts/translations.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  },
  "browser": {
    "fs": false,
    "path": "path-browserify",
    "zlib": "browserify-zlib",
    "stream": "stream-browserify",
    "crypto": "crypto-browserify",
    "http": "stream-http",
    "os": "os-browserify",
    "querystring": "querystring-es3"
  }
